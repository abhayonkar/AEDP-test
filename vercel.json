{
  "version": 2,
  "builds": [
    {
      "src": "aedp_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "env": {
          "LD_LIBRARY_PATH": "/var/task/lib:$LD_LIBRARY_PATH",
          "FONTCONFIG_PATH": "/var/task/.config/fontconfig"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "aedp_project/wsgi.py"
    }
  ]
}
